extern printd(x)

fn fib(x) {
    if x < 3 {
        1
    } else {
        fib(x-1) + fib(x-2)
    }
}

fn fib_iter(x) {
    let a = 1
    let b = 1
    let c
    for let i = 3; i < x; 1 {
        c = a + b
        a = b
        b = c
    }
    b
}

fn main() {
    printd(fib(10))
    printd(fib_iter(10))
}
