use core

fn main() {
    core::printBang()

    let i: int = 0
    loop {
        if i == 1 {
            i += 2
            next
        } else if i == 3 {
            break
        }
        i += 1
        loop {
            i += 1
            core::printInt(i)
            if i == 2 {
                next
            } else {
                break
            }
        }
    }

    core::printDot()
}
