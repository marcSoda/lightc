extern putchar(x: int)

fn print_dot() {
    putchar(46)
    putchar(10)
}

fn print_int(x: int) {
    putchar(x + 48)
    putchar(10)
}

fn print_bang() {
    putchar(33)
    putchar(10)
}

fn print_eq() {
    putchar(61)
    putchar(10)
}

fn main() {
    let x: int = -3
    let y: float = 2.0
    let b: bool = false
    if x == -3 {
        print_eq()
    } else {
        print_dot()
    }

    print_bang()
}
