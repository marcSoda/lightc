extern fn putchar(x: int)

fn printDot() {
    putchar(46)
    putchar(10)
}

fn printInt(x: int) {
    putchar(x + 48)
    putchar(10)
}

fn printBang() {
    putchar(33)
    putchar(10)
}

struct Foo {
    let a: int
    let b: float

    fn bang(o: int) {
        printBang()
    }
}

fn main() {
    let mm: Foo

    mm.bang(1)

    printDot()
}
